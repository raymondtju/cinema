generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movies {
  id           Int    @id @default(autoincrement())
  title        String?
  description  String?
  release_date DateTime? @db.Timestamptz(6)
  poster       String?
  age_rate     Int?
  ticket_price Int?
  created_at   DateTime? @default(now()) @db.Timestamptz(6)
  aired_at     DateTime? @db.Timestamptz(6)
}
